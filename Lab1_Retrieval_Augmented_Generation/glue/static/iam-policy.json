{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "VisualEditor0",
      "Effect": "Allow",
      "Action": [
        "rds:*",
        "ec2:Reject*",
        "firehose:*",
        "redshift:*",
        "quicksight:*",
        "ds:*",
        "ssmmessages:*",
        "iam:PutRolePolicy",
        "codebuild:*",
        "iam:AddRoleToInstanceProfile",
        "states:*",
        "ec2:Associate*",
        "sns:*",
        "iam:Create*",
        "iam:DeleteRole",
        "sts:AssumeRole",
        "codecommit:*",
        "ec2:Modify*",
        "cloudwatch:*",
        "ec2:Monitor*",
        "ec2:Delete*",
        "cloudtrail:*",
        "ec2:Report*",
        "sqs:*",
        "athena:*",
        "ec2:Unmonitor*",
        "iam:PassRole",
        "iam:Get*",
        "ec2:Confirm*",
        "iam:DeleteRolePolicy",
        "kms:*",
        "comprehendmedical:*",
        "lakeformation:*",
        "iam:DeleteInstanceProfile",
        "s3:*",
        "ec2:Copy*",
        "resource-groups:*",
        "iam:ListRoles",
        "ec2:Authorize*",
        "iam:DeleteUser",
        "ec2:Reboot*",
        "ec2:Stop*",
        "es:*",
        "ec2:Describe*",
        "ec2:Disassociate*",
        "ssm:*",
        "iam:PutUserPolicy",
        "codestar:*",
        "ec2:Reset*",
        "logs:*",
        "iam:RemoveRoleFromInstanceProfile",
        "dynamodb:*",
        "ec2:Enable*",
        "iam:AttachRolePolicy",
        "comprehend:*",
        "ec2:Terminate*",
        "iam:DetachRolePolicy",
        "secretsmanager:*",
        "ec2messages:*",
        "config:*",
        "kinesis:*",
        "iam:DetachUserPolicy",
        "events:*",
        "ec2:Get*",
        "ec2:Detach*",
        "iam:AttachUserPolicy",
        "iam:DeleteUserPolicy",
        "cloudformation:*",
        "ec2:Disable*",
        "ec2:Cancel*",
        "ec2:Unassign*",
        "ec2:Search*",
        "translate:*",
        "route53resolver:*",
        "ec2:Attach*",
        "ec2:Move*",
        "ec2:Restore*",
        "ec2:Start*",
        "iam:DeletePolicy",
        "iam:Generate*",
        "glue:*",
        "ec2:Deregister*",
        "ec2:Revoke*",
        "ec2:Accept*",
        "ec2:Import*",
        "ec2:Release*",
        "codedeploy:*",
        "sagemaker:*",
        "tag:*",
        "iam:DeleteLoginProfile",
        "ec2:Bundle*",
        "kinesisanalytics:*",
        "ec2:Create*",
        "ec2:Replace*",
        "elasticloadbalancing:*",
        "cloud9:*",
        "ec2:Assign*",
        "ec2:Register*",
        "route53:*",
        "lambda:*",
        "ec2:Allocate*",
        "codepipeline:*",
        "elasticmapreduce:*"
      ],
      "Resource": "*"
    },
    {
      "Sid": "VisualEditor1",
      "Effect": "Allow",
      "Action": "ec2:RunInstances",
      "Resource": "*",
      "Condition": {
        "StringEquals": {
          "ec2:InstanceType": [
            "t2.nano",
            "t2.micro",
            "t2.small",
            "t3.nano",
            "t3.micro",
            "t3.small"
          ]
        }
      }
    },
    {
      "Sid": "VisualEditor2",
      "Effect": "Allow",
      "Action": "ec2:RunInstances",
      "Resource": [
        "arn:aws:ec2:*::image/ami-*",
        "arn:aws:ec2:*:*:subnet/*",
        "arn:aws:ec2:*:*:key-pair/*",
        "arn:aws:ec2:*:*:instance/*",
        "arn:aws:ec2:*:*:volume/*",
        "arn:aws:ec2:*:*:security-group/*",
        "arn:aws:ec2:*:*:network-interface/*"
      ]
    },
    {
      "Sid": "VisualEditor3",
      "Effect": "Deny",
      "Action": [
        "ec2:DeleteSpotDatafeedSubscription",
        "ec2:RequestSpotInstances",
        "ec2:PurchaseReservedInstancesOffering",
        "ec2:CancelSpotFleetRequests",
        "ec2:PurchaseHostReservation",
        "dynamodb:PurchaseReservedCapacityOfferings",
        "ec2:RequestSpotFleet",
        "rds:PurchaseReservedDBInstancesOffering",
        "ec2:CancelSpotInstanceRequests",
        "ec2:ModifySpotFleetRequest",
        "ec2:CreateSpotDatafeedSubscription",
        "ec2:PurchaseScheduledInstances",
        "ec2:ModifyReservedInstances"
      ],
      "Resource": "*"
    }
	]
}
